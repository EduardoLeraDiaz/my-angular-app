{
  "name": "my-angular-app",
  "version": "0.0.1",
  "description": "my first angularjs app",
  "main": "web.js",
  "dependencies": {
    "angular": "~1.4.9",
    "angular-animate": "^1.5.0",
    "angular-cache": "^4.4.2",
    "angular-formly": "^7.1.2",
    "angular-resource": "^1.4.8",
    "mi-angular-resource-builder": "^0.0.8",
    "mi-angular-wbc-pack": "^0.4.0",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "angular-formly-templates-bootstrap": "^6.1.5",
    "angular-loading-bar": "^0.8.0",
    "angular-sanitize": "~1.4.9",
    "angular-translate": "^2.8.1",
    "angular-translate-loader-partial": "^2.8.1",
    "angular-ui-bootstrap": "^0.14.3",
    "angular-ui-router": "^0.2.15",
    "api-check": "^7.5.5",
    "bootstrap": "^3.3.5",
    "clean-css": "^3.4.6",
    "express": "^4.13.3",
    "font-awesome": "^4.4.0",
    "fs-extra": "^0.26.2",
    "glob": "^6.0.1",
    "jscs": "^2.3.5",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "json-loader": "^0.5.3",
    "less": "^2.5.3",
    "less-plugin-autoprefix": "^1.5.1",
    "ng-annotate-loader": "0.0.10",
    "ng-file-upload": "^10.1.9",
    "npm-font-open-sans-condensed": "0.0.2",
    "npm-font-source-sans-pro": "0.0.3",
    "webpack": "^1.12.2"
  },
  "scripts": {
    "postinstall": "npm run build",
    "prebuild": "mkdir -p web/css/ web/js/ web/img web/fonts/font-awesome web/fonts/source-sans-pro web/fonts/open-sans-condensed",
    "build": "npm run build:css && npm run build:views && npm run build:fonts && npm run build:i18n && npm run jscs && npm run jshint && npm run build:js && npm run build:img",
    "build:js": "webpack --config webpack.config.js",
    "build:css": "lessc --strict-math='on' --autoprefix='last 2 versions,ie 9' src/media/less/style.less web/css/style.css && cleancss --source-map -o web/css/style.min.css web/css/style.css",
    "build:views": "node scripts/copy.js src/**/views/*.html web/views/",
    "build:fonts": "npm run build:font-fontawesome && npm run build:font-sourcesanspro && npm run build:font-opensanscondensed",
    "build:font-fontawesome": "cp -R node_modules/font-awesome/fonts/* web/fonts/font-awesome",
    "build:font-sourcesanspro": "cp -R node_modules/npm-font-source-sans-pro/fonts/* web/fonts/source-sans-pro",
    "build:font-opensanscondensed": "cp -R node_modules/npm-font-open-sans-condensed/fonts/* web/fonts/open-sans-condensed",
    "build:i18n": "node scripts/copy.js src/**/translations/*.json web/i18n/",
    "jscs": "jscs ./src ./tests ./scripts",
    "jshint": "jshint ./src ./tests ./scripts --reporter node_modules/jshint-stylish/index.js",
    "build:img": "cp -R src/media/img/* web/img",
    "start": "node web.js",
    "test": "./node_modules/karma/bin/karma start karma.config.js",
    "test:jenkins": "./node_modules/karma/bin/karma start karma-jenkins.config.js"
  },
  "devDependencies": {
    "angular-mocks": "~1.4.9",
    "david": "^7.0.0",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.11",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "phantomjs": "^1.9.18",
    "watch": "^0.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krimovish/my-angular-app.git"
  },
  "keywords": [
    "angularjs"
  ],
  "author": "krimovish",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krimovish/my-angular-app/issues"
  },
  "homepage": "https://github.com/krimovish/my-angular-app#readme"
}
